{
  "manifest_version": 3,
  "name": "chrest",
  "description": "Manage windows, tabs, and bookmarks via REST",
  "version": "1.1",
  "icons": {
    "128": "chrest_icon_128.png"
  },
  "permissions": [
    "background",
    "identity",
    "idle",
    "history",
    "bookmarks",
    "tabs",
    "devtools",
    "nativeMessaging",
    "management",
    "storage",
    "unlimitedStorage"
  ],
  "background": {
    "service_worker": "main.js",
    "type": "module"
  },
  "options_ui": {
    "page": "options.html"
  }
}
